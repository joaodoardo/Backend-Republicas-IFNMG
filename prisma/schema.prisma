generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  name      String
  email     String  @unique
  password  String
  createdAt DateTime @default(now())
}

model Republica {
  id          Int      @id @default(autoincrement())
  titulo      String
  descricao   String
  bairro      String
  rua         String
  numero      Int
  complemento String
  valorMensal Float
  vagas       Int
  createdAt   DateTime @default(now())
}